program repRSY{
	var double speed = 1.5;

	var int i = 0;

	while (i < 100) {
		do RobotControl.setProperties("RSY RSY", "velocity position", "1.5 -1.35");

		while RobotControl.requiresMotion("RSY"){
			do nothing
		}
	
		do RobotControl.setProperties("RSY RSY", "velocity position", "1.5 0");

		while RobotControl.requiresMotion("RSY"){
			do nothing
		}

		set i = i + 1;
	}	
}
